for pat = 32 % For each participants
    bin_list = [];
    filename = sprintf('TRFDOut_AllPowerBins_ppt%02d.mat',pat);
    load(filename,'data')
    chan_list = []; % (chan, time, freq)
    for chan = 1:32 % For each channel
        bins = data.data; %TODO: change according to the structure
        freq_list = []; % (time, freq);
        for f = 1:5
            freq_list(:,f) =  mean(bins(f,:,:),3);
            disp(size(freq_list))
        end
        chan_list( = [chan_list freq_list];
    end
    pat_list = [pat_list chan_list];
end

